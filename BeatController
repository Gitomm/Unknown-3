using System;
namespace Trial
{
  class IRG
  {
    int a;
    int b;
    private int AssignDim()
    {
      Random _r = new Random();
      a = _r.Next(1,100);
      b = a;
      Console.WriteLine(a);
      Console.WriteLine(b);
    }
    public static void Main(string[] args)
    {
      Console.WriteLine("The Dimensions are " + a * b)
    }
  }
}
